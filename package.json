{
  "name": "aginteractive",
  "version": "0.0.0",
  "scripts": {
    "dev": "rm -rf ./.next && next dev -p 1109",
    "build": "rm -rf ./.next && next build",
    "clean-build": "rm -rf ./.next & rm -rf ./node_nodules && npm install && npm run build && npm start",
    "compile": "tsc --jsx react-jsx --project tsconfig.compile.json && shx mkdir -p ./dist/style && npx postcss ./app/globals.css -o ./dist/style/globals.css",
    "start": "next start -p 1109",
    "docker": "docker-compose down && docker-compose pull && docker-compose build && docker-compose up -d",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prettier": "prettier .",
    "prettier-fix": "prettier --write .",
    "fix": "prettier --write . && next lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesonRGrieve/AGInteractive.git"
  },
  "keywords": [
    "ai",
    "agi",
    "chat",
    "react",
    "nextjs",
    "components"
  ],
  "author": "Jameson R Grieve",
  "bugs": {
    "url": "https://github.com/JamesonRGrieve/AGInteractive/issues"
  },
  "homepage": "http://jamesonrgrieve.github.io/AGInteractive",
